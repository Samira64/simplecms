<% @page_title = "Delete Admin User" %>
<%= link_to("<< Back to List", admin_users_path, :class => 'back_link') %>
<div class="admin_users delete">
    <h2>Delete Admin User</h2>

    <%= form_for(@admin_user, :method => 'delete') do |f| %>

    <p>Are you sure you want to permanantly delete this admin user?</p>
    <p class="reference-name"><%= @admin_user.name %></p>
    <div class="form-buttons">
        <%= f.submit("Delete Admin User") %>
    </div>
    <% end %>

</div>